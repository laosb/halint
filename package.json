{
  "name": "wright",
  "version": "0.0.1",
  "description": "A simple language linter mainly for Chinese.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --reporter spec",
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "lint": "eslint .",
    "pretest": "npm run lint --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laosb/wright.git"
  },
  "keywords": [
    "lint",
    "Chinese"
  ],
  "author": "laosb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laosb/wright/issues"
  },
  "homepage": "https://github.com/laosb/wright#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "mocha": "^2.5.3",
    "should": "^9.0.2"
  },
  "dependencies": {
    "underscore": "^1.8.3"
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ]
  }
}
